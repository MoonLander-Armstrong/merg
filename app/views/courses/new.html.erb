<h1>新增課程</h1>

<%= form_for(@course) do |form| %>

    <div>
        <%= form.label :classImg, "圖片" %>
        <%= form.file_field :classImg %>
    </div>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>
    
    <div>
        <%= form.label :content %>
        <%= form.text_area :content %>
    </div>

    <div>
        <%= form.label :price %>
        <%= form.text_field :price %>
    </div>


    <%= form.submit %>

<% end %>